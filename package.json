{
  "name": "docs",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "cross-env NODE_OPTIONS=--no-webstorage fumadocs-mdx",
    "dev": "cross-env NODE_OPTIONS=--no-webstorage next dev",
    "build": "cross-env NODE_OPTIONS=--no-webstorage fumadocs-mdx && bun run generate:llm && cross-env NODE_OPTIONS=--no-webstorage next build",
    "start": "cross-env NODE_OPTIONS=--no-webstorage next start",
    "generate:llm": "bun run scripts/generate-llm-files.ts",
    "lint": "ultracite check",
    "lint:links": "bun run scripts/lint.ts",
    "lint:fix": "ultracite fix",
    "lint:doctor": "ultracite doctor",
    "format": "ultracite fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@orama/orama": "^3.1.17",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@takumi-rs/core": "^0.57.6",
    "@takumi-rs/helpers": "^0.57.6",
    "@takumi-rs/image-response": "^0.57.6",
    "@takumi-rs/template": "^0.57.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csstype": "^3.2.3",
    "fumadocs-core": "^16.2.5",
    "fumadocs-mdx": "^14.1.1",
    "fumadocs-openapi": "^10.1.4",
    "fumadocs-twoslash": "^3.1.10",
    "fumadocs-ui": "^16.2.5",
    "lucide-react": "^0.561.0",
    "mermaid": "^11.12.2",
    "motion": "^12.23.26",
    "next": "^16.0.10",
    "next-themes": "^0.4.6",
    "next-validate-link": "^1.6.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.20.0",
    "tailwind-merge": "^3.4.0",
    "twoslash": "^0.3.4",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bun": "^1.3.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "lefthook": "^2.0.12",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "ultracite": "^6.4.2"
  },
  "overrides": {
    "shiki": "^3.20.0",
    "@shikijs/core": "^3.20.0"
  }
}
